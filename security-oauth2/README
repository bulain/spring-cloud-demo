http://localhost:8778/oauth/authorize?client_id=client&response_type=code&scope=web&redirect_uri=http://www.baidu.com
curl -X POST -H "Content-Type: application/x-www-form-urlencoded" -d "grant_type=authorization_code&code=code&scope=web&redirect_uri=http://www.baidu.com" "http://client:secret@localhost:8080/oauth/token"


security.oauth2.client.client-id = client
security.oauth2.client.client-secret = secret

